! Transparent background
*depth: 32
!URxvt.transparent: true
!URxvt.shading: 25
!URxvt.tintColor: #FFFFFF
!URxvt.blurRadius: 6

URxvt.intensityStyles:      false
URxvt.font:                 xft:FiraCode Nerd Font Mono:size=14,style=Medium \
                            xft:UbuntuMono Nerd Font, \
                            xft:Inconsolata Nerd Font Mono, \
                            xft:DejaVuSansMono Nerd Font, \
                            xft:DroidSansMono Nerd Font, \
                            xft:FiraMono Nerd Font, \
                            xft:RobotoMono Nerd Font, \
                            xft:Noto Sans Mono, \
                            xft:Monospace:style=Medium:antialias=False
URxvt.scrollBar:            false
URxvt.cursorBlink:          true
#URxvt.cursorColor:          magenta

!! Extensions
URxvt.perl-ext-common:      default,matcher,resize-font,url-select,keyboard-select,selection-to-clipboard
URxvt.colorUL:              #4682B4
!! url-select
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.url-select.launcher:  /usr/bin/xdg-open
URxvt.url-select.underline: true
!! keyboard-select
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller:  C-j
URxvt.resize-font.bigger:   C-k
!! matcher
URxvt.url-launcher:         /usr/bin/xdg-open
URxvt.matcher.buttom:       1
